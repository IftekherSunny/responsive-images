{
    "name" : "brendt/responsive-images",
    "description" : "Create responsive images with PHP, using the srcset and sizes spec.",
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Brent",
            "email" : "brent@pageon.be"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require" : {
        "PHP" : ">=7.1",
        "symfony/finder" : "^3.1",
        "symfony/filesystem" : "^3.1",
        "intervention/image": "^2.3",
        "ps/image-optimizer": "^1.0",
        "amphp/loop": "dev-master@dev",
        "amphp/amp": "2.0.x-dev@dev",
        "amphp/parallel": "0.1.x-dev@dev"
    },
    "require-dev" : {
        "larapack/dd" : "^1.0",
        "phpunit/phpunit" : "^5.0"
    },
    "autoload" : {
        "psr-4" : {
            "Brendt\\Image\\" : "src/",
            "Brendt\\Image\\Tests\\Phpunit\\" : "tests/"
        }
    }
}
